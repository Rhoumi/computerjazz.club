#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bubo/computerjazz.club/node_modules/.pnpm/@11ty+eleventy@3.1.1/node_modules/@11ty/eleventy/node_modules:/Users/bubo/computerjazz.club/node_modules/.pnpm/@11ty+eleventy@3.1.1/node_modules/@11ty/node_modules:/Users/bubo/computerjazz.club/node_modules/.pnpm/@11ty+eleventy@3.1.1/node_modules:/Users/bubo/computerjazz.club/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bubo/computerjazz.club/node_modules/.pnpm/@11ty+eleventy@3.1.1/node_modules/@11ty/eleventy/node_modules:/Users/bubo/computerjazz.club/node_modules/.pnpm/@11ty+eleventy@3.1.1/node_modules/@11ty/node_modules:/Users/bubo/computerjazz.club/node_modules/.pnpm/@11ty+eleventy@3.1.1/node_modules:/Users/bubo/computerjazz.club/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cmd.cjs" "$@"
else
  exec node  "$basedir/../../cmd.cjs" "$@"
fi
